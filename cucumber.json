{
    "default": {
        "defaultTimeoutInterval": 3000000,
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/test/features/*.feature",
            "src/test/features/HomeOffice/*.feature"
        ],
        "publishQuiet": false,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/Hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ]
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": false,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/Hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ]
    }
}